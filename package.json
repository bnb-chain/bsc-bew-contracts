    "ethereum-input-data-decoder": "0.3.0",
    "lodash": ">=4.17.19",
    "mem": ">=4.0.0",
    "minimist": ">=1.2.3",
    "nunjucks": "^3.2.0",
    "openzeppelin-solidity": "v4.2.0",
    "rlp": "2.2.5",
    "solidity-bytes-utils": "0.0.8",
