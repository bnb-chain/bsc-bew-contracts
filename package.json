  "main": "index.js",
  "scripts": {
    "testrpc": "ganache-cli --gasLimit 13000000 -e 100000 --networkId 666",
    "truffle": "truffle",
    "truffle:test": "truffle test",
    "truffle:encode": "truffle test ./test/TestEncodeProxyConstructor.js",
    "truffle:compile": "truffle compile",
    "truffle:migrate": "truffle migrate",
    "coverage": "truffle run coverage --network development",
    "lint": "npm run lint:sol && npm run lint:js",
    "lint:js": "eslint test/**/*.js",
    "lint:sol:fix": "solium -d contracts/ --fix",
    "lint:sol": "solium -d contracts/",
    "flatten": "truffle-flattener contracts/BewSwapImplement.sol | grep -v 'SPDX-License-Identifier' > contracts/flattened/BewSwapImplement.sol && truffle-flattener contracts/BewSwapUpgradeableProxy.sol | grep -v 'SPDX-License-Identifier' > contracts/flattened/BewSwapUpgradeableProxy.sol",
    "generate": "node generate-bew-swap.js",
    "generate-test": "node generate-bew-swap.js --mock true"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "await-sleep": "0.0.1",
